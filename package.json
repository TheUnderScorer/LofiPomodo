{
  "author": {
    "email": "przemyslawzydek@gmail.com",
    "name": "Przemysław Żydek"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "config": {
    "forge": "./forge.config.js"
  },
  "dependencies": {
    "@chakra-ui/core": "1.0.0-rc.8",
    "@chakra-ui/icons": "1.0.0-next.7",
    "@fortawesome/fontawesome-svg-core": "^1.2.32",
    "@fortawesome/free-solid-svg-icons": "^5.15.1",
    "@fortawesome/react-fontawesome": "^0.1.11",
    "auto-launch": "^5.0.5",
    "electron-debug": "^3.1.0",
    "electron-is-dev": "^1.2.0",
    "electron-log": "^4.3.0",
    "electron-squirrel-startup": "^1.0.0",
    "electron-store": "^6.0.0",
    "electron-util": "^0.14.2",
    "emittery": "^0.7.1",
    "framer-motion": "2.9.4",
    "knex": "^0.21.7",
    "lodash.debounce": "^4.0.8",
    "lodash.isplainobject": "^4.0.6",
    "node-fetch": "^2.6.1",
    "ramda": "^0.27.1",
    "react": "^17.0.0",
    "react-beautiful-dnd": "^13.0.0",
    "react-dom": "^17.0.0",
    "react-hook-form": "^6.11.0",
    "react-hotkeys-hook": "^2.4.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.1",
    "react-use": "^15.3.4",
    "recoil": "0.0.13",
    "spectron": "^13.0.0",
    "sqlite3": "^5.0.0",
    "typescript": "^4.1.3",
    "uuid": "^8.3.1"
  },
  "description": "Pomodoro app made with pixel-art like ui.",
  "devDependencies": {
    "@craco/craco": "5.7.0",
    "@electron-forge/cli": "^6.0.0-beta.54",
    "@electron-forge/maker-deb": "^6.0.0-beta.54",
    "@electron-forge/maker-dmg": "^6.0.0-beta.54",
    "@electron-forge/maker-rpm": "^6.0.0-beta.54",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.54",
    "@electron-forge/maker-zip": "^6.0.0-beta.54",
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/exec": "^5.0.0",
    "@semantic-release/git": "^9.0.0",
    "@semantic-release/github": "^7.0.7",
    "@semantic-release/npm": "^7.0.5",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "@types/auto-launch": "^5.0.1",
    "@types/jest": "^24.0.0",
    "@types/lodash.debounce": "^4.0.6",
    "@types/lodash.isplainobject": "^4.0.6",
    "@types/node": "^12.0.0",
    "@types/node-fetch": "^2.5.7",
    "@types/ramda": "^0.27.28",
    "@types/react": "^17.0.0",
    "@types/react-beautiful-dnd": "^13.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-router-dom": "^5.1.6",
    "@types/recoil": "0.0.1",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "4.4.0",
    "@typescript-eslint/parser": "4.4.0",
    "concurrently": "^5.3.0",
    "cross-env": "^7.0.3",
    "electron": "^11.1.1",
    "eslint": "7.11.0",
    "eslint-import-resolver-typescript": "^2.3.0",
    "eslint-plugin-prettier": "^3.1.3",
    "jest-mock-proxy": "^3.0.0",
    "prettier": "^2.1.2",
    "semantic-release": "17.1.2",
    "ts-jest": "26.4.4",
    "ts-loader": "^8.0.4",
    "tsconfig-paths-webpack-plugin": "^3.3.0",
    "webpack-cli": "4.2.0",
    "webpack-node-externals": "^2.5.2"
  },
  "eslintConfig": {
    "extends": "react-app",
    "settings": {
      "react": {
        "version": "detect"
      },
      "import/resolver": {
        "typescript": {}
      }
    },
    "rules": {
      "react/style-prop-object": "off",
      "no-use-before-define": "off",
      "@typescript-eslint/no-use-before-define": "off"
    }
  },
  "homepage": "./",
  "main": "build/electron.js",
  "name": "PixelPomodo",
  "private": true,
  "productName": "PixelPomodo",
  "release": {
    "branches": [
      "+([0-9])?(.{+([0-9]),x}).x",
      "master",
      "next",
      {
        "name": "alpha",
        "prerelease": true
      },
      {
        "name": "beta",
        "prerelease": true
      }
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      [
        "@semantic-release/changelog",
        {
          "changelogFile": "docs/CHANGELOG.md"
        }
      ],
      "@semantic-release/npm",
      [
        "@semantic-release/git",
        {
          "assets": [
            "docs/CHANGELOG.md",
            "package.json",
            "package-lock.json"
          ]
        }
      ],
      [
        "@semantic-release/github"
      ]
    ]
  },
  "resolutions": {
    "**/@typescript-eslint/eslint-plugin": "4.4.0",
    "**/@typescript-eslint/parser": "4.4.0"
  },
  "scripts": {
    "start:renderer": "cross-env EXTEND_ESLINT=true SKIP_PREFLIGHT_CHECK=true craco start",
    "build:renderer": "cross-env EXTEND_ESLINT=true SKIP_PREFLIGHT_CHECK=true craco build",
    "lint": "eslint . --ext .ts,tsx",
    "lint:fix": "eslint . --ext .ts,tsx --fix",
    "test": "SKIP_PREFLIGHT_CHECK=true craco test",
    "test:e2e": "jest --config jest.config.e2e.js --runInBand --forceExit",
    "test:e2e-docker": "docker-compose run e2e",
    "build:electron": "cross-env NODE_ENV=\"production\" webpack --config webpack.electron.js",
    "build:all": "npm run build:renderer && npm run build:electron",
    "watch:electron": "webpack --config webpack.electron.js -w",
    "start:electron": "electron ./build/electron.js --remote-debugging-port=9444",
    "semantic-release": "semantic-release",
    "create-migration": "knex migrate:make -x ts --",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "build-packager": "docker build . -t packager -f docker/packager/Dockerfile --platform linux/amd64",
    "make:arm64-darwin": "electron-forge make --arch=arm64 --platform=darwin",
    "make:arm64-windows": "electron-forge make --arch=arm64 --platform=win32",
    "make:arm64-windows:docker": "docker-compose run packager-arm64-windows",
    "make:arm64-linux": "electron-forge make --arch=arm64 --platform=linux",
    "make:arm64-linux:docker": "docker-compose run packager-arm64-linux",
    "make:x64-darwin": "electron-forge make --arch=x64 --platform=darwin",
    "make:x64-windows": "electron-forge make --arch=x64 --platform=win32",
    "make:x64-windows:docker": "docker-compose run packager-x64-windows",
    "make:x64-linux": "electron-forge make --arch=x64 --platform=linux",
    "make:x64-linux:docker": "docker-compose run packager-x64-linux",
    "rebuild": "electron-rebuild -f -w sqlite3",
    "rebuild:arm64": "electron-rebuild -f -w sqlite3 --arch arm64",
    "rebuild:x64": "electron-rebuild -f -w sqlite3 --arch x64",
    "start-m1-gh-runner": "arch -x86_64 ./actions-runner/run.sh"
  },
  "version": "1.7.1"
}
